# Ignore Python virtual environments
venv/
.venv/

# Ignore Python bytecode
__pycache__/
*.pyc
*.pyo
*.pyd

# Ignore build artifacts
dist/
build/
*.egg-info/
gyrosi_babylm.egg-info/

# Ignore logs and reports
logs/
*.log
flake8_report.txt

# Ignore OS files
.DS_Store

# Ignore IDE/editor settings
.vscode/
.idea/
*.swp

# Ignore binary and large data files
*.dat
*.bin
*.icns

# Ignore documentation build output
docs/

# Ignore test fixtures and generated test data
scripts/tests/
scripts/tests/fixtures/
scripts/tests/*.dat

# Ignore migration scripts and temporary scripts
scripts/migrate_*.py

# Ignore any secret or config files (add more as needed)
*.key
*.pem
*.env

# Ignore AI helper config itself
.aichignore

# Explicitly ignore root-level metadata/config
.git/
.editorconfig
.flake8
.gitattributes
.gitignore
.pre-commit-config.yaml
.mypy_cache/
.pytest_cache/
.coverage

# Project-specific exclusions
# BabyLM new structure: ignore all thread and key data, but keep directory structure
memories/private/*/threads/
memories/private/*/keys/
!memories/private/
!memories/private/*/
!memories/private/*/threads/
!memories/private/*/keys/

# Ignore htmlcov coverage output
htmlcov/
